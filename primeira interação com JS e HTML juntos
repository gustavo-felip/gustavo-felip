et titulo = document.querySelector ("h1");
titulo.innerHTML = "Hora do Desafio.";

function exibirMensagem(){
    console.log("botão clicado!");
};
function mensagem(){
alert('Eu amo JS!');
};
function pergunta(){
    let cidade = prompt ("qual a sua cidade?")
    alert (`estive em ${cidade} e lembrei de você!`)
}
function soma(){
let numero1 = parseInt(prompt("digite um numero inteiro"));
let numero2 = parseInt(prompt("digite um segundo numero inteiro"));
let resultado = numero1 + numero2;
alert (`o ${numero1} mais o ${numero2} é igual ${resultado}`);
};
